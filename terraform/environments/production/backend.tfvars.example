# Cloudflare R2 Backend Configuration
# Copy this file to backend.tfvars and fill in your values.
# NEVER commit backend.tfvars to source control!
#
# Usage: terraform init -backend-config=backend.tfvars

# R2 API Token credentials (create at Cloudflare Dashboard → R2 → Manage R2 API Tokens)
access_key = ""
secret_key = ""

# R2 endpoint (replace with your Cloudflare account ID)
# Find your account ID in the Cloudflare dashboard URL or account overview
endpoints = {
  s3 = "https://<CLOUDFLARE_ACCOUNT_ID>.r2.cloudflarestorage.com"
}
